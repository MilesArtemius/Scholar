<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<VBox prefHeight="200.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="classes.windows.SignIn">
   <children>

      <StackPane prefHeight="50.0" prefWidth="200.0">
         <children>
            <Text fx:id="dialog_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Lorem ipsum dolor sit amet!">
                <font>
                    <Font name="System Bold" size="24.0" />
                </font>
            </Text>
         </children>
      </StackPane>

      <HBox alignment="CENTER_LEFT" prefHeight="37.5" prefWidth="200.0">
         <children>
            <Text fx:id="courage_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Lorem ipsum dolor sit amet, consectetur adipiscing elit:">
                <HBox.margin>
                    <Insets left="10.0" />
                </HBox.margin>
                <font>
                    <Font size="13.0" />
                </font>
            </Text>
         </children>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>

      <HBox alignment="CENTER_LEFT" prefHeight="37.5" prefWidth="200.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="37.5" prefWidth="100.0">
               <children>
                  <Text fx:id="login_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Lorem" />
               </children>
            </HBox>
            <TextField fx:id="login_field" prefWidth="400.0" />
         </children>
      </HBox>

      <HBox alignment="CENTER_LEFT" prefHeight="37.5" prefWidth="200.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="37.5" prefWidth="100.0">
               <children>
                  <Text fx:id="password_text" strokeType="OUTSIDE" strokeWidth="0.0" text="Ipsum" />
               </children>
            </HBox>
            <TextField fx:id="password_field" prefWidth="400.0" />
         </children>
      </HBox>

      <HBox alignment="TOP_RIGHT" prefHeight="37.5" prefWidth="200.0">
         <children>
            <Button fx:id="submit_button" mnemonicParsing="false" text="Consectetur" />
         </children>
      </HBox>
       
   </children>
   <padding>
      <Insets left="20.0" right="20.0" />
   </padding>
</VBox>
