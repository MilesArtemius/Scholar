<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<?import javafx.scene.control.TextField?>
<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="classes.windows.TreeBuilder">
   <children>
      <StackPane prefHeight="40.0" prefWidth="570.0">
         <children>
            <Text fx:id="header" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
         </children>
      </StackPane>
      <HBox prefHeight="310.0" prefWidth="570.0">
         <children>
            <TreeView fx:id="tree" prefHeight="350.0" prefWidth="410.0">
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin></TreeView>
            <StackPane prefHeight="350.0" prefWidth="150.0">
               <children>
                  <Text fx:id="desc" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
               </children>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </StackPane>
         </children>
      </HBox>
      <HBox prefHeight="50.0" prefWidth="570.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="50.0" prefWidth="500.0" maxWidth="500">
               <children>
                  <TextField fx:id="path" prefWidth="500.0" maxWidth="500" text="Text" />
               </children>
               <HBox.margin>
                  <Insets right="5.0" />
               </HBox.margin>
            </HBox>
            <HBox alignment="CENTER_RIGHT" prefHeight="50.0" prefWidth="60.0">
               <children>
                  <Button fx:id="submit_button" mnemonicParsing="false" text="Button" />
               </children>
               <HBox.margin>
                  <Insets left="5.0" />
               </HBox.margin>
            </HBox>
         </children>
      </HBox>
   </children>
   <padding>
      <Insets left="15.0" right="15.0" />
   </padding>
</VBox>
